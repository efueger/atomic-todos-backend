    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "exists": "^1.0.0",
    "express": "^4.14.0",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
    "uppercamelcase": "^1.1.0",
    "winston": "^2.2.0"
